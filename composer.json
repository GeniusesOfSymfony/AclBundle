{
    "name": "nuxia/acl-manager-bundle",
    "type": "symfony-bundle",
    "description": "ACL Manager for Symfony2",
    "keywords": ["ACL"],
    "homepage": "https://github.com/nuxia/AclManagerBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Stobbe",
            "email": "djstobbe@gmail.com"
        },
        {
            "name": "Nuxia",
            "email": "contact@nuxia.fr"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "2.*",
        "symfony/security-bundle": "2.*"
    },
    "suggest": {
        "symfony/doctrine-bundle": "*",
        "symfony/mongodb-odm-bundle": "*",
        "doctrine/couchdb-odm-bundle": "*"
    },
    "autoload": {
        "psr-4": { "Problematic\\AclManagerBundle\\": "" }
    },
    "autoload-test":{
        "psr-4": { "Problematic\\AclManagerBundle\\Tests\\": "" }
    },
    "require-dev": {
        "doctrine/orm": "~2.4",
        "doctrine/doctrine-bundle": "1.*",
        "symfony/finder": "~2.3",
        "phpunit/phpunit": "~4.0",
        "symfony/browser-kit": "~2.3"
    }
}
